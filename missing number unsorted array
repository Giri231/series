#include<iostream>

using namespace std;

int main()
{
    int n,sum=0;
    cin>>n;
    int a[n];
    for(int i=0;i<n;i++)
    {
        cin>>a[i];
    }
  
    for(int i=0;i<n;i++)
    {
        sum =sum+a[i];
    }
    n=n+1;
      int t=(n*(n+1))/2;
      
    int z=t-sum;
    cout<<z;
    
    
}
